{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*"
        },
        {
          "url": "https://*"
        }
      ]
    },
    "mobile-sharetarget:default",
    "core:default",
    "clipboard-manager:allow-read-text",
    "os:allow-os-type",
    "opener:default",
    "tts:allow-speak",
    "tts:allow-stop",
    "tts:allow-set-voice",
    "tts:allow-get-all-voices",
    "safe-area-insets-css:default",
    "http:default"
  ],
  "platforms": ["android", "iOS"]
}
